    "def Davies_Bouldin(Hom, Sep2_2):\n",
    "    k = Sep2_2.shape[0] # nb de clusters\n",
    "    db_max = 0 # initialisation de l'indice de Davies Bouldin\n",
    "    for ic1 in range(k):\n",
    "        for ic2 in range(ic1+1, k):\n",
    "            db_cur = ( Hom[ic1] + Hom[ic2] ) / Sep2_2[ic1,ic2]\n",
    "            if db_cur &gt; db_max:\n",
    "                db_max = db_cur\n",
    "    #print(db_max)\n",
    "    return db_max/k\n",
    "\n",
    "\n"
   ]
